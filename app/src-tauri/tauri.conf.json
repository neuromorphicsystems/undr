{
    "$schema": "../node_modules/@tauri-apps/cli/schema.json",
    "build": {
        "beforeBuildCommand": "cross-env MODE=production npm run build",
        "beforeDevCommand": "cross-env MODE=development npm run build",
        "devPath": "../build",
        "distDir": "../build"
    },
    "package": {
        "productName": "Undr",
        "version": "0.1.0"
    },
    "tauri": {
        "allowlist": {
            "dialog": {
                "message": true,
                "open": true,
                "save": true
            },
            "os": {
                "all": true
            },
            "shell": {
                "open": true
            }
        },
        "bundle": {
            "active": true,
            "category": "DeveloperTool",
            "copyright": "",
            "deb": {
                "depends": []
            },
            "externalBin": [],
            "icon": [
                "icons/32x32.png",
                "icons/128x128.png",
                "icons/128x128@2x.png",
                "icons/icon.icns",
                "icons/icon.ico"
            ],
            "identifier": "com.icns.undr",
            "longDescription": "",
            "macOS": {
                "entitlements": null,
                "exceptionDomain": "",
                "frameworks": [],
                "providerShortName": null,
                "signingIdentity": null
            },
            "resources": [],
            "shortDescription": "",
            "targets": "all",
            "windows": {
                "certificateThumbprint": null,
                "digestAlgorithm": "sha256",
                "timestampUrl": ""
            }
        },
        "security": {
            "csp": null
        },
        "updater": {
            "active": false
        },
        "windows": [
            {
                "fullscreen": false,
                "height": 800,
                "minHeight": 420,
                "minWidth": 350,
                "resizable": true,
                "title": "UNDR",
                "visible": false,
                "width": 600
            }
        ]
    }
}
