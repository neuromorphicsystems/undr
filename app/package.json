{
    "name": "undr",
    "productName": "Undr",
    "description": "GUI to manage UNDR datasets",
    "author": {
        "name": "Alexandre Marcireau",
        "email": "a.marcireau@gmail.com"
    },
    "version": "0.1.0",
    "dependencies": {
        "react": "17.0.2",
        "react-dom": "17.0.2"
    },
    "devDependencies": {
        "@svgr/core": "5.5.0",
        "@svgr/plugin-jsx": "5.5.0",
        "@svgr/plugin-svgo": "5.5.0",
        "@types/react": "17.0.2",
        "@types/react-dom": "17.0.2",
        "@types/styled-components": "5.1.14",
        "@vitejs/plugin-react-refresh": "1.3.6",
        "cross-env": "7.0.3",
        "electron": "13.2.1",
        "electron-builder": "22.11.7",
        "electron-store": "8.0.0",
        "esbuild": "0.12.26",
        "prettier": "2.3.2",
        "styled-components": "5.3.0",
        "typescript": "4.3.5",
        "vite": "2.5.0"
    },
    "scripts": {
        "watch": "cross-env MODE=development node scripts.js watch",
        "release": "cross-env MODE=production node scripts.js release",
        "lint": "prettier --write ."
    },
    "prettier": {
        "tabWidth": 4,
        "arrowParens": "avoid"
    },
    "main": "build/main/main.js",
    "build": {
        "appId": "au.edu.icns.undr",
        "files": [
            "package.json",
            {
                "from": "build/main",
                "to": "main"
            },
            {
                "from": "build/renderer",
                "to": "renderer"
            }
        ],
        "directories": {
            "output": "build/electron",
            "buildResources": "."
        },
        "mac": {
            "darkModeSupport": true,
            "identity": false
        }
    }
}
