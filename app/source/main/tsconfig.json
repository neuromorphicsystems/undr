{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "isolatedModules": true,
        "jsx": "react",
        "lib": ["ESNext"],
        "module": "esnext",
        "moduleResolution": "Node",
        "skipLibCheck": true,
        "sourceMap": true,
        "strict": true,
        "target": "esnext",
        "types": ["node", "vite/client"],
        "typeRoots": ["./node_modules/@types"]
    }
}
